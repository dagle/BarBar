<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="BarBarBar" id="window1">
    <child>
      <object class="GtkActionBar">
        <child type="start">
          <object class="BarBarRiverTag">
          </object>
        </child>
        <child type="center">
          <object class="BarBarRiverView">
          </object>
        </child>
        <child type="end">
          <object class="BarBarClock">
            <property name="format">%k:%M:%S</property>
            <property name="interval">1000</property>
          </object>
        </child>
        <child type="end">
          <!-- <property name="interval">1000</property> -->
          <object class="BarBarMem">
            <child>
              <object class="BarBarLabel">
                <property name="format">{percentage:.0}% ïƒ‰</property>
              </object>
            </child>
          </object>
        </child>
        <child type="end">
          <object class="BarBarCpu">
            <child>
              <object class="BarBarDiagram">
                <property name="value">load</property>
              </object>
            </child>
            <!-- <property name="interval">1000</property> -->
          </object>
        </child>
        <child type="end">
          <object class="BarBarBattery">
            <!-- <property name="interval">1000</property> -->
          </object>
        </child>
        <child type="end">
          <object class="BarBarNetwork">
            <!-- wlan0: -->
            <property name="interface">wlan0</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
